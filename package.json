{
  "name": "toml-it",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "dependencies": {
    "glob": "^8.0.3",
    "toml": "^3.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "files": [
    "./bin/*",
    "./lib/*"
  ],
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node ./lib/",
    "test": "jest --silent ./src/framework.spec.ts",
    "test:runner": "tsc; jest ./src/toml/runner.spec.ts",
    "coverage": "jest --coverage --silent",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prettier": "prettier --write ./src/**/*.ts",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "jest": "^29.3.0",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3"
  }
}
